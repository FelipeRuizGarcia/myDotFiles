# LINUX ALIAS
# keyboard
alias us="setxkbmap us"
alias latam="setxkbmap latam"

# apt manager
alias ins="apt-get install -y"

# rsa key
alias keypub="cat ~/.ssh/id_rsa.pub"

# alias
alias load="source ~/.bashrac"

# files
alias del="rm -rf "

# dirs
alias c="cd .."
alias cd..="cd .."

# network
alias proxy="env | grep proxy"
